<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Assert.IsFalse</Title>
      <Shortcut>af</Shortcut>
      <Description>Code snippet for an Assert.IsFalse().</Description>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>Microsoft.VisualStudio.TestTools.UnitTesting</Namespace>
        </Import>
      </Imports>
      <References>
        <Reference>
          <Assembly>Microsoft.VisualStudio.TestPlatform.TestFramework.dll</Assembly>
        </Reference>
      </References>
      <Declarations>
        <Literal>
          <ID>condition</ID>
          <ToolTip>Replace with the name of the condition to test against</ToolTip>
          <Default>condition</Default>
        </Literal>
      </Declarations>      
      <Code Language="csharp">
      <![CDATA[Assert.IsFalse($condition$);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>