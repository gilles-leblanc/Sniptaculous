<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>Assert.IsNull</Title>
      <Shortcut>an</Shortcut>
      <Description>Code snippet for an Assert.IsNull().</Description>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>Microsoft.VisualStudio.TestTools.UnitTesting</Namespace>
        </Import>
      </Imports>
      <References>
        <Reference>
          <Assembly>Microsoft.VisualStudio.QualityTools.UnitTestFramework.dll</Assembly>
        </Reference>
      </References>
       <Declarations>
        <Literal>
          <ID>value</ID>
          <ToolTip>Replace with the name of the value to test against</ToolTip>
          <Default>value</Default>
        </Literal>
      </Declarations>      
      <Code Language="csharp">
      <![CDATA[Assert.IsNull($value$);$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
